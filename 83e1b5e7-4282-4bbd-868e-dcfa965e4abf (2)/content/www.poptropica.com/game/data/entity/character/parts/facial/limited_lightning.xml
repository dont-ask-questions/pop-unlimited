<part>
	<id>limited_lightning</id>
	<type>facial</type>
	<costumizable>false</costumizable>
	<notPrintable>true</notPrintable>
	<hide>
		<part>hair</part>
	</hide>
	<specialAbility>
		<className>game.data.specialAbility.character.InvisiblePower</className>
		<useButton>true</useButton>
		<triggerable>true</triggerable>
		<parameters>
			<param id="tracking">IncrediblesQuest,invisible</param>
		</parameters>
	</specialAbility>

	<components>
		<component class="game.components.entity.character.part.FlipPart">
			<properties>
				<instanceData class="game.data.character.part.InstanceData">
					<constructor>
						<instanceName>flipClip</instanceName>
					</constructor>
				</instanceData>
			</properties>
		</component>
	</components>
</part>