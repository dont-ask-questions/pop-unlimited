<scene>
	<sceneType>default</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>3000</right>
		<bottom>2100</bottom>	
	</cameraLimits>
	<bounds>
		<left>-100</left>
		<top>0</top>
		<right>3100</right>
		<bottom>2000</bottom>
	</bounds>
	<data>dialog.xml,items.xml,doors.xml,npcs.xml,sounds.xml,hits.xml</data>
	<absoluteFilePaths>scenes/survival5/shared/dialog.xml</absoluteFilePaths>
	<assets></assets>
	<layers>
		<layer>
			<asset>backdrop.swf</asset>
			<rate>.75</rate>
		</layer>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>75</x>
			<y>1800</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>