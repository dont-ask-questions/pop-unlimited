<scene>
	<sceneType>default</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>2000</right>
		<bottom>1000</bottom>	
	</cameraLimits>
	<bounds>
		<left>-100</left>
		<top>0</top>
		<right>2100</right>
		<bottom>920</bottom>
	</bounds>
	<data>sounds.xml</data>
	<assets></assets>
	<layers>

		<layer event="set_night">
			<asset>backgroundNight.swf</asset>
			<rate>1</rate>
		</layer>
		<layer event="set_evening">
			<asset>backgroundEvening.swf</asset>
			<rate>1</rate>
		</layer>
		<layer event="set_day">
			<asset>backgroundDay.swf</asset>
			<rate>1</rate>
		</layer>

		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>view3d.swf</asset>
			<rate>1</rate>
		</layer>

	</layers>

</scene>