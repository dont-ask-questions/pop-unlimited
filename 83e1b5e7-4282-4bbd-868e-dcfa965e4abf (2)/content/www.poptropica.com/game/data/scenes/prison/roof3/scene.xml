<scene>
	<sceneType>default</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>42</top>
		<right>3800</right>
		<bottom>1000</bottom>	
	</cameraLimits>
	<bounds>
		<left>-50</left>
		<top>45</top>
		<right>3800</right>
		<bottom>1000</bottom>
	</bounds>
	<data>dialog.xml,doors.xml,npcs.xml,sounds.xml,hits.xml,photos.xml</data>
	<absoluteFilePaths>scenes/prison/shared/dialog.xml</absoluteFilePaths>
	<assets></assets>
	<layers>
		<layer>
			<asset>backdrop.swf</asset>
			<rate>.75</rate>
		</layer>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>200</x>
			<y>700</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>