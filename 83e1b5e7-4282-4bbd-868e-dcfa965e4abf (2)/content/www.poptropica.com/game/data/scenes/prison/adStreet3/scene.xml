<scene>
	<sceneType>billboard</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>1900</right>
		<bottom>1080</bottom>	
	</cameraLimits>
	<bounds>
		<left>-100</left>
		<top>0</top>
		<right>2000</right>
		<bottom>1040</bottom>
	</bounds>
	<data>custom.xml,doors.xml,hits.xml,sounds.xml</data>
	<assets></assets>
	<absoluteFilePaths prependTypePath="true">scenes/prison/shared/dialog.xml</absoluteFilePaths>
	<layers>
		<layer>
			<asset>backdrop.swf</asset>
			<rate>.75</rate>
		</layer>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>20</x>
			<y>1040</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>