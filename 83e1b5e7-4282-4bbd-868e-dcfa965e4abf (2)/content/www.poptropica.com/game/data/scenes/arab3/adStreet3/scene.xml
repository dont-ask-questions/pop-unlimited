<scene>
	<sceneType>billboard</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>1920</right>
		<bottom>1080</bottom>	
	</cameraLimits>
	<bounds>
		<left>50</left>
		<top>0</top>
		<right>1870</right>
		<bottom>1030</bottom>
	</bounds>
	<data>custom.xml,doors.xml,hits.xml,sounds.xml</data>
	<assets></assets>
	<absoluteFilePaths prependTypePath="true">scenes/arab3/shared/dialog.xml</absoluteFilePaths>
	<layers>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>200</x>
			<y>1000</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>