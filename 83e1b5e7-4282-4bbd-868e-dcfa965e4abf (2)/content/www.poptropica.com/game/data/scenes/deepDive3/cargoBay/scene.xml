<scene>
	<sceneType>shortmain</sceneType>
	<cameraLimits>
		<left>0</left>
		<top>0</top>
		<right>2120</right>
		<bottom>1300</bottom>	
	</cameraLimits>
	<bounds>
		<left>0</left>
		<top>0</top>
		<right>2320</right>
		<bottom>1300</bottom>
	</bounds>
	<data>dialog.xml,doors.xml,sounds.xml,hits.xml</data>
	<absoluteFilePaths prependTypePath="true">scenes/deepDive3/shared/sounds.xml</absoluteFilePaths>
	<assets></assets>
	<layers>
		<layer>
			<asset>background.swf</asset>
			<rate>1</rate>
		</layer>
		<layer event="spoke_with_ai">
			<asset>backgroundS3.swf</asset>
			<rate>1</rate>
			<condition type="alwaysMerge">background,placeOver</condition>
		</layer>
		<layer hit="true">
			<asset>interactive.swf</asset>
			<rate>1</rate>
		</layer>
		<layer>
			<asset>foreground.swf</asset>
			<rate>1</rate>
		</layer>
	</layers>
	<player>
		<defaultPosition>
			<x>700</x>
			<y>600</y>
		</defaultPosition>
		<defaultDirection>right</defaultDirection>
	</player>
</scene>