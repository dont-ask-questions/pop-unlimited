<hits>
	<hit id="floor" color="0x00ff00" platform="true">
		<component type="platform"/>
	</hit>
	<hit id="top" color="0x006600" platform="true">
		<component type="platform"/>
	</hit>
	<hit id="log" platform="true">
		<component type="movingPlatform">
	 		<velocity>0</velocity>
	 		<loop>false</loop>
		</component>
	</hit>
	<hit id="wall" color="0xff6600">
		<component type="wall"/>
	</hit>
	<hit id="water" color="0x0000ff">
		<component type="water">
			<splashColor1>0x65DCFE</splashColor1>
			<splashColor2>0xA7EBFE</splashColor2>
			<density>0.8</density>
			<viscosity>.96</viscosity>
		</component>
		<component type="mover">
			<!-- If true, platforms have higher priority so you'll stick to them even in water. -->
			<stickToPlatforms>false</stickToPlatforms>
		</component>
	</hit>
</hits>