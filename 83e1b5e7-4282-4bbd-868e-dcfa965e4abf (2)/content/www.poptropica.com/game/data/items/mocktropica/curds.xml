<item id="curds">
	<type>island</type>
	<subType>mocktropica</subType>
	
	<textfields>
  		<text>
    		<value>Cheese Curds</value>
  		</text>
  		<text id="islandgray">
  			<value>Press SPACEBAR to throw a curd!</value>
  			<width>190</width>
  			<y>90</y>
  		</text>
  	</textfields>
	
	<assets>
  		<asset id="cardBack">
       		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-35</saturation>
     			<hue>-4</hue>
    		</effect>
	    </asset>
	    	<asset id="cardContent"/>
    </assets>

	<buttons>
		<btn>
			<conditional id="hasLook" isTrue="false"></conditional>
    		<label>EQUIP</label>
    		<actions>
	    		<action type="applyLook"/>
			</actions>
  		</btn>
  		<btn>
			<conditional id="hasLook" isTrue="true"></conditional>
    		<label>REMOVE</label>
    		<actions>
	    		<action type="removeLook"/>
			</actions>
  		</btn>
	</buttons>
	
	<looks>
		<look>
			<item>mk_curds</item>
		</look>
	</looks>
	
	<conditionals>
		<conditional type="hasLook"/>
	</conditionals>
</item>