<item id="garbanzo_man_head">
	<type>island</type>
	<subType>timmy</subType>
	<textfields>
 		<text>
      		<value>Garbanzo Man Mask</value>
    	</text>
  	</textfields>
	<assets>
  		<asset id="cardBack">
  			<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-73</saturation>
     			<hue>170</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
    
    <buttons>
  		<btn>
			<conditional id="hasLook" isTrue="false"></conditional>
    		<label>PUT ON</label>
    		<actions>			  	
	    		<action type="applyLook"/>
			</actions>
  		</btn>
  		<btn>
			<conditional id="hasLook" isTrue="true"></conditional>
    		<label>REMOVE</label>
    		<actions>
	    		<action type="removeLook"/>
			</actions>
  		</btn>
	</buttons>
	
    <conditionals>
		<conditional type="hasLook"/>
    </conditionals>
	
	<looks>
		<look>
			<facial>tf_garbanzo_man_head</facial>
		</look>
	</looks>
</item>