<item id="notebook">
	<type>island</type>
	<subType>time</subType>
	<textfields>
		<text id="title">
    		<value>Notebook</value>
  		</text>
    	<text>
      		<value>This notebook contains a lot of inventive drawings.</value>
      		<size>20</size>
      		<y>105</y>
    	</text>
  	</textfields>
	<assets>
  		<asset id="cardBack">
    		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>3</brightness>
     			<contrast>0</contrast>
     			<saturation>-40</saturation>
     			<hue>20</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
</item>
