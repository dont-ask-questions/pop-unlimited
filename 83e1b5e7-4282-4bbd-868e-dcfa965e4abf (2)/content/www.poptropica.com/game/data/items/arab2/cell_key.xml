<item id="cell_key">
	<type>island</type>
	<subType>arab2</subType>
	<textfields>
 		<text>
      		<value>Prison Key</value>
    	</text>
  	</textfields>

	<assets>
  		<asset id="cardBack">
			<assetPath>items/arab1/background.swf</assetPath>
       		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-67</saturation>
     			<hue>46</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
	
	<buttons>
		<btn>
    		<label>USE</label>
    		<actions>
	    		<action type="triggerEvent">
		  			<parameters>
		  				<param id="event">use_cell_key</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
</item>