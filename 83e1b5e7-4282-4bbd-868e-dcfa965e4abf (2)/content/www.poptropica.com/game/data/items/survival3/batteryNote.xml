<item id="batteryNote">
	<type>island</type>
	<subType>survival3</subType>
	
	<textfields>
  		<text>
    		<value>Note</value>
  		</text>
  	</textfields>
  	
	<assets>
  		<asset id="cardBack">
  			<assetPath>items/survival1/background.swf</assetPath>
  			<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-80</saturation>
     			<hue>25</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
    
    <buttons>
		<btn>
    		<label>EXAMINE</label>
    		<actions>
				<action type="openPopup">
		  			<parameters>
		  				<param id="className">game.scenes.survival3.shared.popups.BatteryNotePopup</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
</item>