<item id="salt"> <!-- Make sure the id name is the same as your .xml and the .swf art you want to load on the card -->
	<type>island</type>
	<subType>carnival</subType> <!-- Set subType to the location of card .swf art - bin/assets/items/"folder" -->
	
	<textfields>
 		<text>
      		<value>Salt</value>
    	</text>
  	</textfields>
	
	<assets>
  		<asset id="cardBack">
			<assetPath>items/carnival/background.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-62</saturation>
     			<hue>14</hue>
    			</effect>
	    	</asset>
	    	<asset id="cardContent"/>
    </assets>
	
</item>