<item id="key"> <!-- Make sure the id name is the same as your .xml and the .swf art you want to load on the card -->
  	<type>island</type>
  	<subType>deepDive1</subType> <!-- Set subType to the location of card .swf art - bin/assets/items/"folder" -->

  	<textfields>
   	 	<text>
      		<value>Key</value>
   	 	</text>
  	</textfields>
		
	<assets>
  		<asset id="cardBack">
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
       		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-56</saturation>
     			<hue>17</hue>
    			</effect>
	    	</asset>
	    	<asset id="cardContent"/>
    </assets>
  	
	<buttons>
  		<btn>
	    	<label>USE</label>
	    	<actions>
				<action type="triggerEvent">
					<parameters>
						<param id="event">used_key</param>
					</parameters>
				</action>
			</actions>
  		</btn>
  	</buttons>
	
   
</item>
