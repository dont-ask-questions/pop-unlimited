<item id="fish_files">  
	<type>island</type>
	<subType>deepDive1</subType>
	
	<textfields>
		<text>
    		<value>Sea Creature Files</value>
  		</text>
  	</textfields>
	
	<assets>
  		<asset id="cardBack">
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>-5</brightness>
     			<contrast>0</contrast>
     			<saturation>-56</saturation>
     			<hue>8</hue>
    			</effect>
	    	</asset>
	    	<asset id="cardContent"/>
    </assets>
	
	<buttons>
		<btn>
    		<label>EXAMINE</label>
    		<actions>
				<action type="openPopup">
		  			<parameters>
		  				<param id="className">game.scenes.deepDive1.shared.FishPopup</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
	
</item>
