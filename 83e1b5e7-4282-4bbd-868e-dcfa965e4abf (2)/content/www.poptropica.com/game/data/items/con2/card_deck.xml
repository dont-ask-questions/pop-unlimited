<item id="card_deck">
	<type>store</type>
	<subType>general</subType>
	<buttons>
  		<btn>
    		<label>ORGANIZE</label>
    		<actions>
	    		<action type="openPopup">
		  			<parameters>
		  				<param id="className">game.scenes.con2.shared.popups.CardDeck</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
	<textfields>
 		<text>
      		<value>Card Deck</value>
    	</text>
  	</textfields>
	<assets>
  		<asset id="cardBack">
  			<assetPath>items/con1/background.swf</assetPath>
