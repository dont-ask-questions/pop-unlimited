<item id="sasha_card">
	<type>island</type>
	<subType>con2</subType>
	<textfields>
 		<text>
      		<value>Sasha's Calling Card</value>
    	</text>
  	</textfields>
  	<buttons>
		<btn>
    		<label>CALL NUMBER</label>
    		<actions>
	    		<action type="triggerEvent">
		  			<parameters>
		  				<param id="event">call_sasha_phone</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
	<assets>
  		<asset id="cardBack">
  			<assetPath>items/con2/background.swf</assetPath>
 			<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>3</brightness>
     			<contrast>0</contrast>
     			<saturation>-20</saturation>
     			<hue>80</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent">
  			<assetPath>items/con2/sasha_card.swf</assetPath>
    	</asset>
    </assets>
</item>