<item id="falafel">
	<type>island</type>
		<subType>virusHunter</subType>
	<buttons>
		<btn>
    		<label>USE</label>
    		<actions>
	    		<action type="triggerEvent">
		  			<parameters>
		  				<param id="event">useFalafel</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
	<textfields>
  		<text>
    		<value>Falafel</value>
    		<size>32</size>
  		</text>
  	</textfields>
	<assets>
  		<asset id="cardBack">
       		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-64</saturation>
     			<hue>-144</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
</item>
