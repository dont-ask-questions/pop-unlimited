<item id="petriDish">
	<type>island</type>
	<subType>virusHunter</subType>
	<buttons>
		<btn>
			<label>USE</label>
			<actions>
	    		<action type="triggerEvent">
		  			<parameters>
		  				<param id="event">usePetriDish</param>
					</parameters>
				</action>
			</actions>
  		</btn>
	</buttons>
	<textfields>
  		<text>
    		<value>Petri Dish</value>
  		</text>
  	</textfields>
	<assets>
  		<asset id="cardBack">
       		<effect>
      			<name>adjustColorFilter</name>
      			<enabled>true</enabled>
      			<brightness>0</brightness>
     			<contrast>0</contrast>
     			<saturation>-38</saturation>
     			<hue>-10</hue>
    		</effect>
    	</asset>
    	<asset id="cardContent"/>
    </assets>
</item>