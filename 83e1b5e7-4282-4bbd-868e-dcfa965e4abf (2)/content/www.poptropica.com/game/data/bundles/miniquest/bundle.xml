<bundle>
	<id>miniquest</id>
	<name>MiniQuest Pack</name>
	
	   <cardSets>
				 <cardSet id="shared">
					<card>3244</card>
					<card>3081</card>
					<card>3109</card>
					<card>3100</card>
					<card>3223</card>
					<card>3231</card>
					<card>3232</card>
					<card>3336</card>
					<card>7000</card>
                </cardSet>
        </cardSets>
	
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>12</saturation>
	   			<hue>156</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/explorers.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>