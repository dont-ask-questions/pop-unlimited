<bundle>
	<id>pets</id>
	<name>Pets Pack</name>
	
	   <cardSets>
               <cardSet id="shared">
					<card>3463</card>
					<card>3464</card>
					<card>3465</card>
                </cardSet>
                
        </cardSets>
	
	
	<assets>
  		<asset id="bundleBase">
			<assetPath>bundles/bundle_base.swf</assetPath>
			<!-- These effect settings change the color of the background card, you get the numbers from Flash's "Adjust Color" filter -->
			<effect>
	    		<name>adjustColorFilter</name>
	    		<enabled>true</enabled>
	    		<brightness>0</brightness>
	   			<contrast>0</contrast>
	   			<saturation>12</saturation>
	   			<hue>156</hue>
    		</effect>
	    </asset>
	 	<asset id="bundleIcon">
	    	<assetPath>bundles/pets.swf</assetPath>
	    </asset> 
    </assets>
    
</bundle>